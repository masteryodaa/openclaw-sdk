{
  "mcpServers": {
    "openclaw-docs": {
      "type": "stdio",
      "command": "python",
      "args": ["-m", "openclaw_sdk.mcp.docs_server"],
      "description": "Search and browse OpenClaw SDK documentation"
    },
    "openclaw-sdk": {
      "type": "stdio",
      "command": "python",
      "args": ["-m", "openclaw_sdk.mcp.sdk_server"],
      "env": {
        "OPENCLAW_URL": "ws://127.0.0.1:18789"
      },
      "description": "Interact with a live OpenClaw gateway"
    }
  }
}
